<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ConfirmBuyController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#222222" height="600.0" stroke="BLACK" strokeType="INSIDE" width="800.0" />
      <Label fx:id="user_txt" layoutX="175.0" layoutY="241.0" prefHeight="34.0" prefWidth="514.0" text="Username" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="t_txt" layoutX="175.0" layoutY="284.0" prefHeight="34.0" prefWidth="514.0" text="theatre" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="movie_txt" layoutX="175.0" layoutY="326.0" prefHeight="34.0" prefWidth="514.0" text="movie" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="chair_txt" layoutX="175.0" layoutY="412.0" prefHeight="34.0" prefWidth="514.0" text="chair" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="time_txt" layoutX="175.0" layoutY="369.0" prefHeight="34.0" prefWidth="514.0" text="time" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ImageView fx:id="poster_img" fitHeight="210.0" fitWidth="216.0" layoutX="295.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="confirm_btn" layoutX="232.0" layoutY="513.0" mnemonicParsing="false" onAction="#handleGoConfirm_btnOnAction" prefHeight="55.0" prefWidth="126.0" style="-fx-background-color: #00ea0b;" text="Confirm!">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="cancel_btn" layoutX="403.0" layoutY="513.0" mnemonicParsing="false" onAction="#handleGoCancel_btnOnAction" prefHeight="55.0" prefWidth="126.0" style="-fx-background-color: #ff1414;" text="Cancel">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Label fx:id="price_txt" layoutX="175.0" layoutY="456.0" prefHeight="34.0" prefWidth="514.0" text="price" textFill="RED">
         <font>
            <Font name="System Bold Italic" size="31.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
